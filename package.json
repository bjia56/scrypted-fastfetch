{
   "name": "@bjia56/scrypted-fastfetch",
   "version": "0.2.15",
   "description": "Fastfetch system information tool for Scrypted",
   "license": "Apache",
   "keywords": [
      "scrypted",
      "plugin",
      "fastfetch",
      "neofetch"
   ],
   "scripts": {
      "scrypted-setup-project": "scrypted-setup-project",
      "prescrypted-setup-project": "scrypted-package-json",
      "build": "cd src/webcomponent && npm run build && cd ../../ && scrypted-webpack",
      "prepublishOnly": "NODE_ENV=production cd src/webcomponent && npm run build && cd ../../ && scrypted-webpack",
      "prescrypted-vscode-launch": "scrypted-webpack",
      "scrypted-vscode-launch": "scrypted-deploy-debug",
      "scrypted-deploy-debug": "scrypted-deploy-debug",
      "scrypted-debug": "scrypted-debug",
      "scrypted-deploy": "scrypted-deploy",
      "scrypted-readme": "scrypted-readme",
      "scrypted-package-json": "scrypted-package-json"
   },
   "scrypted": {
      "name": "fastfetch",
      "type": "API",
      "interfaces": [
         "DeviceProvider",
         "StreamService",
         "TTYSettings",
         "Settings",
         "HttpRequestHandler",
         "WebComponentProvider"
      ],
      "interfaceDescriptors": {
         "WebComponentProvider": {
            "name": "WebComponentProvider",
            "methods": [
               "getWebComponentClass"
            ],
            "properties": []
         }
      }
   },
   "devDependencies": {
      "@scrypted/sdk": "^0.5.52",
      "@types/adm-zip": "^0.5.5",
      "coffee-loader": "^5.0.0"
   },
   "dependencies": {
      "adm-zip": "^0.5.14"
   }
}
